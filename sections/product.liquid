<script defer>
      class VariantSelector extends HTMLElement {
    constructor() {
      super();
      this.addEventListener('click', this.onVariantClick.bind(this));
    }

    onVariantClick(e) {
      const clickedLi = e.target.closest('.variant-value');
      if (!clickedLi) return;

      const parentUl = clickedLi.closest('.variant-values');
      if (!parentUl) return;

      // Remove active class from all siblings
      parentUl.querySelectorAll('.variant-value').forEach((li) => li.classList.remove('active'));
      clickedLi.classList.add('active');

      // Proceed with variant logic
      this.getSelectedOptions();
      this.getSelectedVariant();

      if (this.currentVariant) {
        this.updateURL();
        this.updateFormID();
        this.updatePrice();
      }
    }

    getSelectedOptions() {
      this.options = Array.from(this.querySelectorAll('.variant-values'), (ul) => {
        const activeLi = ul.querySelector('.variant-value.active');
        return activeLi ? activeLi.dataset.value : null;
      }).filter(Boolean);

      console.log('Selected options:', this.options);
    }

    getVariantJSON() {
      this.variantData = this.variantData || JSON.parse(this.querySelector('[type="application/json"]').textContent);
      return this.variantData;
    }

    getSelectedVariant() {
      const variants = this.getVariantJSON();
      this.currentVariant = variants.find((variant) =>
        variant.options.every((opt, index) => opt === this.options[index])
      );

      console.log('Selected Variant:', this.currentVariant);
    }

    updateURL() {
      if (!this.currentVariant) return;
      window.history.replaceState({}, '', `${this.dataset.url}?variant=${this.currentVariant.id}`);
    }

    updateFormID() {
      const formInput = document.querySelector('#product-form')?.querySelector('input[name="id"]');
      if (formInput && this.currentVariant) {
        formInput.value = this.currentVariant.id;
      }
    }

    updatePrice() {
      if (!this.currentVariant) return;
      fetch(`${this.dataset.url}?variant=${this.currentVariant.id}&section_id=${this.dataset.section}`)
        .then((response) => response.text())
        .then((responseText) => {
          const html = new DOMParser().parseFromString(responseText, 'text/html');
          const oldPrice = document.getElementById('price-{{ section.id }}');
          const newPrice = html.getElementById('price-{{ section.id }}');

          const oldPerkg = document.querySelector(".perKg");
          const newPerkg = html.querySelector(".perKg");

          if (oldPrice && newPrice) oldPrice.innerHTML = newPrice.innerHTML;
          if (oldPerkg && newPerkg) oldPerkg.innerHTML = newPerkg.innerHTML;
        });
    }
      }

      customElements.define('variant-selector', VariantSelector);
</script>

<script>
      $(document).ready(function () {
        $('.prod-main-image').slick({
          slidesToShow: 1,
          slidesToScroll: 1,
          arrows: false,
          fade: true,
          asNavFor: '.prod-thumb-image',
        });
        $('.prod-thumb-image').slick({
          slidesToShow: 4,
          slidesToScroll: 1,
          asNavFor: '.prod-main-image',
          focusOnSelect: true,
          centerMode: true,
          vertical: true,
          arrows: false,
        });
        $(".custom-prev").click(function () {
          $(".prod-main-image").slick("slickPrev");
        });

        $(".custom-next").click(function () {
          $(".prod-main-image").slick("slickNext");
        });
        $(".custom-prev").addClass("slick-disabled");
        $(".prod-main-image").on("afterChange", function () {
          if ($(".slick-prev").hasClass("slick-disabled")) {
            $(".custom-prev").addClass("slick-disabled");
          } else {
            $(".custom-prev").removeClass("slick-disabled");
          }
          if ($(".slick-next").hasClass("slick-disabled")) {
            $(".custom-next").addClass("slick-disabled");
          } else {
            $(".custom-next").removeClass("slick-disabled");
          }
        });
      });
</script>

<script>
    $(document).ready(function(){
      $(document).on('click', '.pa-ques-wrap', function() {
        const $clickedItem = $(this).closest('.pa-item');     // Find parent pa-item
        const $clickedAnswer = $clickedItem.find('.pa-div');  // Find its sibling pa-div

        // If already active, collapse it
        if ($clickedItem.hasClass('active')) {
          $clickedItem.removeClass('active');
          $clickedAnswer.slideUp(300);
          return;
        }

        // Otherwise, deactivate all others first
        $('.pa-item').removeClass('active');
        $('.pa-div').slideUp(300);

        // Then activate the clicked one
        $clickedItem.addClass('active');
        $clickedAnswer.slideDown(300);
      });
    });

</script>

